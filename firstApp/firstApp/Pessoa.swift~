import UIKit

class Pessoa: NSObject {

    let nome: String
    let idade: Int
    let altura: Double
    
    init(nome: String, idade: Int, altura: Double) {
        self.nome = nome
        self.idade = idade
        self.altura = altura
    }
    
}
